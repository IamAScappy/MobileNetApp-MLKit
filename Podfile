source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.0'
use_frameworks!


def my_pods
    
    # [START base_dependencies]
    pod 'Firebase/Core'
    pod 'Firebase/MLVision'
    # [END base_dependencies]
    pod 'Firebase/MLModelInterpreter'

end


target 'MobileNetApp-MLKit' do
    my_pods
end
target 'MobileNetApp-MLKitUITests' do
    my_pods
end

